{
  "name": "nest-next-module",
  "version": "0.5.3",
  "description": "using nestjs and nextjs together!",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ananiy/nest-next-module.git"
  },
  "keywords": [
    "nestjs",
    "nextjs",
    "nestjs-module"
  ],
  "author": "ananiy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ananiy/nest-next-module/issues"
  },
  "homepage": "https://github.com/ananiy/nest-next-module#readme",
  "dependencies": {
    "@nestjs/common": "^6.10.8",
    "@nestjs/core": "^6.10.8",
    "@types/express": "^4.17.2",
    "express": "^4.17.1",
    "next": "^9.1.5"
  },
  "devDependencies": {
    "@types/node": "^12.12.16",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && tsc -p tsconfig.check.json"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --check",
      "eslint",
      "git add"
    ]
  }
}
